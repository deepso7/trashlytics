{
  "name": "trashlytics",
  "type": "module",
  "version": "0.1.0",
  "description": "A lightweight event tracking library",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "rolldown -c && tsc -p tsconfig.build.json && cp dist/index.d.ts dist/index.d.cts",
    "test": "vitest",
    "check": "ultracite check",
    "fix": "ultracite fix",
    "prepublishOnly": "pnpm run build",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm run build && changeset publish"
  },
  "keywords": [
    "analytics",
    "tracking",
    "events",
    "telemetry"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/deepso7/trashlytics"
  },
  "packageManager": "pnpm@10.26.2",
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@changesets/cli": "2.29.8",
    "@total-typescript/ts-reset": "0.6.1",
    "@types/node": "25.0.3",
    "rolldown": "1.0.0-beta.57",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "ultracite": "6.5.0",
    "vitest": "4.0.16"
  },
  "dependencies": {
    "effect": "3.19.13"
  }
}
